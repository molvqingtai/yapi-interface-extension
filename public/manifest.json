{
  "name": "Yapi-Interface-Extension",
  "manifest_version": 2,
  "version": "1.0.0",
  "author": "molvqingtai",
  "description": "在 YAPI 接口文档中显示 TypeScript Interface",
  "icons": {
    "64": "icon-64.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/main.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["inject-scripts/*.js"]
}
